#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//usr/bin/node" ]; then
  exec "$basedir//usr/bin/node"  "$basedir/../acquit-require/bin/acquit-require.js" "$@"
else 
  exec /usr/bin/node  "$basedir/../acquit-require/bin/acquit-require.js" "$@"
fi
