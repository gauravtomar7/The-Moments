{
  "name": "acquit-ignore",
  "version": "0.2.1",
  "description": "Acquit plugin for removing lines of code from output",
  "main": "index.js",
  "scripts": {
    "test": "NODE_PATH=../ mocha --harmony test/*",
    "test-coverage": "NODE_PATH=../ istanbul cover _mocha -- --harmony -R spec ./test/*"
  },
  "author": "Valeri Karpov <val@karpov.io>",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/vkarpov15/acquit-ignore.git"
  },
  "peerDependencies": {
    "acquit": ">=0.3.0"
  },
  "dependencies": {
    "archetype": "0.13.0"
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requirePaddingNewLinesAfterBlocks": null,
    "requireMultipleVarDecl": null,
    "requireTrailingComma": null
  },
  "devDependencies": {
    "acquit": "1.x",
    "istanbul": "0.3.5",
    "mocha": "5.x"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
